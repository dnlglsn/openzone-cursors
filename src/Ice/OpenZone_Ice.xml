<theme name="OpenZone_Ice">
  <source size="32" gridsize="32" margin="2" spacing="4">
    <image file="hotspots.png" use="hotspot"/>
    <image file="0.png" use="0"/>
    <image file="1.png" use="1"/>
    <image file="2.png" use="2"/>
    <image file="3.png" use="3"/>
    <image file="4.png" use="4"/>
    <image file="5.png" use="5"/>
    <image file="6.png" use="6"/>
    <image file="7.png" use="7"/>
    <image file="8.png" use="8"/>
    <image file="9.png" use="9"/>
    <image file="10.png" use="10"/>
    <image file="11.png" use="11"/>
    <image file="12.png" use="12"/>
    <image file="13.png" use="13"/>
  </source>

  <layout>
    <row>
      <cursor name="left_ptr"/>
      <cursor name="whats_this"/>
      <cursor name="copy"/>
      <cursor name="move"/>
      <cursor name="link"/>
      <cursor name="forbidden"/>
      <cursor name="left_ptr_watch">
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
      </cursor>
      <cursor name="wait">
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
      </cursor>
    </row>
    <row>
      <cursor name="right_ptr"/>
      <cursor name="pointing_hand"/>
      <cursor name="openhand"/>
      <cursor name="closedhand"/>
      <cursor name="ibeam"/>
      <cursor name="vertical-text"/>
      <cursor name="cross"/>
      <cursor name="size_all"/>
    </row>
    <row>
      <cursor name="size_hor"/>
      <cursor name="size_ver"/>
      <cursor name="size_fdiag"/>
      <cursor name="size_bdiag"/>
      <cursor name="up_arrow"/>
      <cursor name="down_arrow"/>
      <cursor name="right_arrow"/>
      <cursor name="left_arrow"/>
    </row>
    <row>
      <cursor name="pencil"/>
      <cursor name="color-picker"/>
      <cursor name="zoom-in"/>
      <cursor name="zoom-out"/>
      <cursor name="cell"/>
      <cursor name="dot"/>
      <cursor name="dotbox"/>
      <cursor name="X_cursor"/>
    </row>
    <row>
      <cursor name="pirate"/>
    </row>
  </layout>

  <!-- complete to full Qt specification -->
  <alias name="split_v" target="size_ver"/>
  <alias name="split_h" target="size_hor"/>

  <!-- complete to full freedesktop.org specification -->
  <alias name="default" target="left_ptr"/>
  <alias name="help" target="whats_this"/>
  <alias name="alias" target="link"/>
  <alias name="no-drop" target="forbidden"/>
  <alias name="not-allowed" target="forbidden"/>
  <alias name="progress" target="left_ptr_watch"/>
  <alias name="pointer" target="pointing_hand"/>
  <alias name="all-scroll" target="size_all"/>
  <alias name="col-resize" target="size_hor"/>
  <alias name="e-resize" target="size_hor"/>
  <alias name="w-resize" target="size_hor"/>
  <alias name="ew-resize" target="size_hor"/>
  <alias name="row-resize" target="size_ver"/>
  <alias name="n-resize" target="size_ver"/>
  <alias name="s-resize" target="size_ver"/>
  <alias name="ns-resize" target="size_ver"/>
  <alias name="nw-resize" target="size_fdiag"/>
  <alias name="se-resize" target="size_fdiag"/>
  <alias name="nwse-resize" target="size_fdiag"/>
  <alias name="ne-resize" target="size_bdiag"/>
  <alias name="sw-resize" target="size_bdiag"/>
  <alias name="nesw-resize" target="size_bdiag"/>
  <alias name="crosshair" target="cross"/>
  <alias name="text" target="ibeam"/>

  <!-- gtk dnd-* cursors -->
  <alias name="dnd-none" target="left_ptr"/>
  <alias name="dnd-ask" target="whats_this"/>
  <alias name="dnd-copy" target="copy"/>
  <alias name="dnd-move" target="move"/>
  <alias name="dnd-link" target="link"/>
  <alias name="dnd-no-drop" target="forbidden"/>

  <!-- add some standard X.Org names -->
  <alias name="arrow" target="left_ptr"/>
  <alias name="question_arrow" target="whats_this"/>
  <alias name="circle" target="forbidden"/>
  <alias name="watch" target="wait"/>
  <alias name="draft_large" target="right_ptr"/>
  <alias name="draft_small" target="right_ptr"/>
  <alias name="grabbing" target="closedhand"/>
  <alias name="hand1" target="pointing_hand"/>
  <alias name="hand2" target="pointing_hand"/>
  <alias name="fleur" target="size_all"/>
  <alias name="sizing" target="size_all"/>
  <alias name="sb_h_double_arrow" target="size_hor"/>
  <alias name="right_side" target="size_hor"/>
  <alias name="left_side" target="size_hor"/>
  <alias name="sb_v_double_arrow" target="size_ver"/>
  <alias name="top_side" target="size_ver"/>
  <alias name="bottom_side" target="size_ver"/>
  <alias name="based_arrow_up" target="size_ver"/>
  <alias name="based_arrow_down" target="size_ver"/>
  <alias name="top_left_corner" target="size_fdiag"/>
  <alias name="bottom_right_corner" target="size_fdiag"/>
  <alias name="top_right_corner" target="size_bdiag"/>
  <alias name="bottom_left_corner" target="size_bdiag"/>
  <alias name="center_ptr" target="up_arrow"/>
  <alias name="sb_up_arrow" target="up_arrow"/>
  <alias name="sb_down_arrow" target="down_arrow"/>
  <alias name="sb_right_arrow" target="right_arrow"/>
  <alias name="sb_left_arrow" target="left_arrow"/>
  <alias name="cross_reverse" target="crosshair"/>
  <alias name="diamond_cross" target="crosshair"/>
  <alias name="xterm" target="ibeam"/>
  <alias name="plus" target="cell"/>
  <alias name="draped_box" target="dotbox"/>
  <alias name="icon" target="dotbox"/>
  <alias name="target" target="dotbox"/>

  <!-- other -->
  <alias name="crossed_circle" target="forbidden"/>
  <alias name="half-busy" target="left_ptr_watch"/>
  <alias name="5c6cd98b3f3ebcb1f9c7f1c204630408" target="whats_this"/>
  <alias name="d9ce0ab605698f320427677b458ad60b" target="whats_this"/>
  <alias name="1081e37283d90000800003c07f3ef6bf" target="copy"/>
  <alias name="6407b0e94181790501fd1e167b474872" target="copy"/>
  <alias name="b66166c04f8c3109214a4fbd64a50fc8" target="copy"/>
  <alias name="3085a0e285430894940527032f8b26df" target="link"/>
  <alias name="640fb0e74195791501fd1ed57b41487f" target="link"/>
  <alias name="a2a266d0498c3104214a47bd64ab0fc8" target="link"/>
  <alias name="03b6e0fcb3499374a867c041f52298f0" target="forbidden"/>
